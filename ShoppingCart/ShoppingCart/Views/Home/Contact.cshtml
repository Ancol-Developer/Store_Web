@model ContactModel
<div class="row">
    <div class="col-sm-5 col-md-5">
        @Html.Raw(Model.Map)
        @Html.Raw(Model.Description)
    </div>
    <div class="col-sm-4">
        <div class="address">
            <h5>Name: @Model.Name</h5>
            <h5>Name: @Model.Email</h5>
            <h5>Name: @Model.Phone</h5>
        </div>
    </div>
    <div class="col-sm-4">
        <div class="fb-page" 
        data-href="https://www.facebook.com/kccshopvn" 
        data-tabs="message" data-width="" 
        data-height="" 
        data-small-header="false" 
        data-adapt-container-width="true" 
        data-hide-cover="false" 
        data-show-facepile="true"><blockquote cite="https://www.facebook.com/kccshopvn" class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/kccshopvn">Kccshop</a></blockquote></div>
    </div>
</div>